{
    "name": "Game portal",
    "description": "Game portal to create rooms for gamers",
    "keywords": ["game", "games", "portal", "game portal", "gamers"],
    "authors": [
        {
            "name": "Sergey Podgornyy",
            "email": "sergey.podgornyy@yahoo.de",
            "homepage": "http://digit.co.nf/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "monolog/monolog": "1.*",
        "slim/slim": "2.*",
        "propel/propel": "2.0.0-alpha5",
        "validator/livr": "dev-master",
        "components/jquery": "2.*",
        "components/bootstrap": "*",
        "there4/slim-test-helpers": "*",
        "tedivm/jshrink": "~1.0",
        "tuupola/slim-basic-auth": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "config": {
        "component-dir": "public/static/vendor",
        "component-baseurl": "/static"
    },
    "autoload": {
        "classmap": ["lib"],
        "files": []
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/WebbyLab/php-validator-livr.git"
        }
    ]
}
